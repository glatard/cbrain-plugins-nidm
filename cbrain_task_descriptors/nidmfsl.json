{
  "name": "nidmfsl",
  "tool-version": "latest",
  "description": "A python library to export FSL's feat results to NIDM-Results http://nidm.nidash.org/specs/nidm-results.html",
  "command-line": "nidmfsl [FEAT_DIR] [OUTPUT]",
  "docker-image": "glatard/nidmfsl",
  "docker-index": "http://index.docker.io",
  "schema-version": "0.2",
  "inputs": [
    {
      "id": "feat_dir",
      "name": "Feat directory",
      "type": "File",
      "description": "Feat directory to export.",
      "command-line-key": "[INPUT]"
    },
    {
      "id": "output_name",
      "name": "Output name",
      "type": "String",
      "description": "Name of the NIDM output directory",
      "command-line-key": "[OUTPUT]",
      "optional": true,
      "command-line-flag": "-o",
      "default-value": "nidm"
    }
  ],
  "output-files": [
    {
      "id": "output",
      "name": "NIDM directory",
      "description": "Exported NIDM directory",
      "path-template": "[OUTPUT]"
    }
  ]
}
